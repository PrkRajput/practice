<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
	xmlns="http://webservice.GetApplicationCustomFieldAnswersResponse.ais/"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:common="http://ais.co.uk/xsd/jam/message/AisCommonTypes"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
	targetNamespace="http://webservice.GetApplicationCustomFieldAnswersResponse.ais/"
	elementFormDefault="qualified" version="$Revision: 1.2 $" jaxb:version="1.0"
	jaxb:extensionBindingPrefixes="xjc">
	<xs:annotation>
		<xs:appinfo>
			<jaxb:schemaBindings>
				<jaxb:package name="ais.getApplicationCustomFieldAnswers.response" />
				<jaxb:nameXmlTransform>
					<jaxb:typeName prefix="Xml" suffix="VO" />
					<jaxb:elementName prefix="Xml" suffix="VO" />
				</jaxb:nameXmlTransform>
			</jaxb:schemaBindings>
		</xs:appinfo>
	</xs:annotation>
	
	<xs:import namespace="http://ais.co.uk/xsd/jam/message/AisCommonTypes"
		schemaLocation="AisCommonTypes.xsd" />
	
	<xs:element name="GetApplicationCustomFieldAnswersResponse" type="GetApplicationCustomFieldAnswersResponseType" />
	
	<xs:complexType name="GetApplicationCustomFieldAnswersResponseType">
		<xs:choice>
			<xs:element name="Success" type="SuccessType">
				<xs:annotation>
					<xs:documentation>Block indicates a successful completion
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Failure" type="FailureType">
				<xs:annotation>
					<xs:documentation>Block indicates a not successful completion
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	
	<xs:complexType name="SuccessType">
		<xs:sequence>
			<xs:element name="CustomFieldsDetails" type="CustomFieldsDetailsType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomFieldsDetailsType">
		<xs:sequence>
			<xs:element name="Channel" type="ChannelType" />
			<xs:element name="Category" type="CategoryType" />
			<xs:element name="Society" type="common:SocietyType" minOccurs="1" />
			<xs:element name="Mortgage" type="common:String20Type" />
			<xs:element name="ApplSeq" type="common:ApplSeqType" />
			<xs:element name="CustomFields" type="CustomFieldsType"	maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomFieldsType">
		<xs:sequence>
			<xs:element name="CustomField" type="CustomFieldType"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomFieldType">
		<xs:sequence>
			<xs:element name="CustomFieldSeqNo" type="common:String12Type"
				minOccurs="0" />
			<xs:element name="CustomFieldCode" type="common:String4Type"
				minOccurs="0" />
			<xs:element name="CustomFieldPositionNo" type="common:String2Type"
				minOccurs="0" />
			<xs:element name="CustomFieldText" type="common:String255Type"
				minOccurs="0" />
			<xs:element name="CustomFieldType" type="common:String9Type"
				minOccurs="0" />
			<xs:element name="CustomFieldLength" type="common:String3Type"
				minOccurs="0" />
			<xs:element name="EffectiveDate" type="xs:date" minOccurs="0" />
			<xs:element name="PreConfiguredAnswers" type="PreConfiguredAnswersType"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PreConfiguredAnswersType">
		<xs:sequence>
			<xs:element name="Answer" type="AnswerType" maxOccurs="unbounded" />
			<xs:element name="ApplicationAnswers" type="ApplicationAnswersType"
				maxOccurs="unbounded" />

		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="ApplicationAnswersType">
		<xs:sequence>
			<xs:element name="ClientId" type="common:String12Type"
				minOccurs="0" />
			<xs:element name="AnswerText" type="common:String255Type"
				minOccurs="0" />
			<xs:element name="AnswerSeqNo" type="common:String12Type"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="AnswerType">
		<xs:sequence>
			<xs:element name="AnswerSeqNo" type="common:String12Type"
				minOccurs="0" />
			<xs:element name="AnswerPositionNo" type="common:String2Type"
				minOccurs="0" />
			<xs:element name="AnswerText" type="common:String255Type"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="ChannelType">
		<xs:sequence>
			<xs:element name="Code" type="common:String2Type"
				minOccurs="0" />
			<xs:element name="Description" type="common:String30Type"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CategoryType">
		<xs:sequence>
			<xs:element name="Code" type="common:String4Type"
				minOccurs="0" />
			<xs:element name="Description" type="common:String30Type"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>



	<xs:complexType name="WarningType">
		<xs:sequence>
			<xs:element name="Message" type="xs:string" />
			<xs:element name="Code" type="xs:int" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="WarningsType">
		<xs:sequence>
			<xs:element name="Warning" type="WarningType" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="FailureType">
		<xs:sequence>
			<xs:element name="Errors" type="ErrorsType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ErrorsType">
		<xs:sequence>
			<xs:element name="Error" type="ErrorType" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ErrorType">
		<xs:sequence>
			<xs:element name="Timestamp" type="xs:dateTime" />
			<xs:element name="Message" type="xs:string" />
			<xs:element name="Code" type="xs:int" minOccurs="0" />
			<xs:element name="Diagnostic" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
</xs:schema>
